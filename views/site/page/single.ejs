<!DOCTYPE html>
<html>

<head>
    <title>Pet Store - Cửa hàng thú cưng trực tuyến</title>
    <!-- for-mobile-apps -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <script type="application/x-javascript">
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <!--//for-mobile-apps -->
    <!--Custom Theme files -->
    <link href="customer/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
    <link href="customer/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!--//Custom Theme files -->
    <!--js-->
    <script src="customer/js/jquery-1.11.1.min.js"></script>
    <script src="customer/js/modernizr.custom.js"></script>
    <!--//js-->
    <!--flex slider-->
    <script defer src="customer/js/jquery.flexslider.js"></script>
    <link rel="stylesheet" href="customer/css/flexslider1.css" type="text/css" media="screen" />
    <script>
        // Can also be used with $(document).ready()
$(window).load(function() {
  $('.flexslider').flexslider({
    animation: "slide",
    controlNav: "thumbnails"
  });
});
</script>
    <!--flex slider-->
    <script src="customer/js/imagezoom.js"></script>
    <!--cart-->
    <!-- <script src="customer/js/simpleCart.min.js"></script> -->
    <!--cart-->
    <!--web-fonts-->
    <link href='//fonts.googleapis.com/css?family=Raleway:400,100,100italic,200,200italic,300,400italic,500,500italic,600,600italic,700,700italic,800,800italic,900,900italic'
        rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic'
        rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Pompiere' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Fascinate' rel='stylesheet' type='text/css'>
    <!--web-fonts-->
    <!--animation-effect-->
    <link href="customer/css/animate.min.css" rel="stylesheet">
    <script src="customer/js/wow.min.js"></script>
    <script>
        new WOW().init();
    </script>
    <!--//animation-effect-->
    <!--start-smooth-scrolling-->
    <script type="text/javascript" src="customer/js/move-top.js"></script>
    <script type="text/javascript" src="customer/js/easing.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({
                    scrollTop: $(this.hash).offset().top
                }, 1000);
            });
        });
    </script>
    <!--//end-smooth-scrolling-->
</head>

<body>
    <!--header-->
    <% include ../layout/header.ejs %>

    <!--breadcrumbs-->
    <div class="breadcrumbs">
        <div class="container">
            <ol class="breadcrumb breadcrumb1 animated wow slideInLeft" data-wow-delay=".5s">
                <li><a href="trang-chu"><span class="glyphicon glyphicon-home" aria-hidden="true"></span>Trang chủ</a></li>
                <li class="active">Chi tiết sản phẩm</li>
            </ol>
        </div>
    </div>
    <!--//breadcrumbs-->
    <!--single-page-->
    <div class="single">
        <div class="container">
            <div class="single-info">
                <div class="col-md-6 single-top wow fadeInLeft animated" data-wow-delay=".5s">
                    <div class="flexslider">
                        <ul class="slides">
                            <li data-thumb="upload/<%= data.img%>">
                                <div class="thumb-image"> <img src="upload/<%= data.img%>" data-imagezoom="true" class="img-responsive"
                                        alt=""> </div>
                            </li>
                            <li data-thumb="upload/<%= data.img%>">
                                <div class="thumb-image"> <img src="upload/<%= data.img%>" data-imagezoom="true" class="img-responsive"
                                        alt=""> </div>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-6 single-top-left simpleCart_shelfItem wow fadeInRight animated" data-wow-delay=".5s">
                    <h6 class="item_price">
                        <%=data.name%></h6>
                    <p>
                    <div class="single-rating">
                        <span class="starRating">
                            <input id="rating5" type="radio" name="rating" value="5" checked>
                            <label for="rating5">5</label>
                            <input id="rating4" type="radio" name="rating" value="4">
                            <label for="rating4">4</label>
                            <input id="rating3" type="radio" name="rating" value="3">
                            <label for="rating3">3</label>
                            <input id="rating2" type="radio" name="rating" value="2">
                            <label for="rating2">2</label>
                            <input id="rating1" type="radio" name="rating" value="1">
                            <label for="rating1">1</label>
                        </span>
                        <p>5.00 out of 5</p>
                        <a href="#">Add Your Review</a>
                    </div>
                    <%if( data.promotion <= 0 ) { %>
                                <h6 class="item_price" style="color:#d8703f;">
                        <%=data.price%> VNĐ</h6>
                                <% } else { %>
                                <h6 class="item_price" style="color:#d8703f;">
                                    <p>
                                    Giá: 
                                    <del><%=data.price%> VNĐ</del>
                                    <span class="item_price" id="price<%=data.price%>" style="color: #FF590F;"><%=(data.price)-((data.price) * ((data.promotion) /100))%> VNĐ</span>
                                </p>
                                </h6>
                                <% } %>
                    
                    <p>
                        <%=data.des%>
                    </p>
                    <div class="clearfix"> </div>
                    <div class="quantity">
                        <p class="qty"> Qty : <%=data.quantity%></p>
                    </div>
                    <div class="btn_form">
                        <a href="/add-cart.<%= data._id %>" class="add-cart item_add">Thêm vào giỏ hàng</a>
                    </div>
                </div>
                <div class="clearfix"> </div>
            </div>
            <!--collapse-tabs-->

            <div class="grid_3 grid_5 wow fadeInDown animated animated"  data-wow-delay=".5s" style="visibility: visible; animation-delay: 0.5s; animation-name: fadeInDown;">
                <div class="bs-example bs-example-tabs" role="tabpanel" data-example-id="togglable-tabs">
                    <ul id="myTab" class="nav nav-tabs" role="tablist">
                        <li role="presentation" class="active"><a href="#home" id="home-tab" role="tab" data-toggle="tab"
                                aria-controls="home" aria-expanded="true">Mô tả sản phẩm</a></li>
                        <li role="presentation" class=""><a href="#profile" role="tab" id="profile-tab" data-toggle="tab"
                                aria-controls="profile" aria-expanded="false">Địa chỉ liên hệ</a></li>
                    </ul>
                    <div id="myTabContent" class="tab-content">
                        <div role="tabpanel" class="tab-pane fade active in" id="home" aria-labelledby="home-tab">
                            <p>
                                <%=data.des%>
                            </p>
                        </div>
                        <div role="tabpanel" class="tab-pane fade" id="profile" aria-labelledby="profile-tab">
                            <div style="width: 100%">
                                <iframe width="100%" height="200" src="https://maps.google.com/maps?width=100%&amp;height=600&amp;hl=en&amp;q=%20Cafe%20%C4%90%E1%BB%93ng%20Hoa%20158%20%2F29A%20Nguy%E1%BB%85n%20Vi%E1%BB%87t%20H%E1%BB%93ng%20Qu%E1%BA%ADn%20Ninh%20Ki%E1%BB%81u%20Xu%C3%A2n%20Kh%C3%A1nh%20Ninh%20Ki%E1%BB%81u%20C%E1%BA%A7n%20Th%C6%A1%2C%20Vietnam+(C%E1%BB%ADa%20h%C3%A0ng)&amp;ie=UTF8&amp;t=&amp;z=18&amp;iwloc=B&amp;output=embed"
                                    frameborder="0" scrolling="no" marginheight="0" marginwidth="0"><a href="https://www.maps.ie/create-google-map/">Embed
                                        Google Map</a></iframe></div><br />
                        </div>
                    </div>
                </div>
            </div>
            <!--related-products-->
            <div class="related-products">
                <div class="title-info wow fadeInUp animated" data-wow-delay=".5s">
                    <h3 class="title"><span> Sản phẩm cùng loại</span></h3>
                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit curabitur </p>
                </div>
                <div class="related-products-info">
                    <% product.forEach(function(pro){ %>
                    <div class="col-md-3 new-grid simpleCart_shelfItem wow flipInY animated"  style="margin-right: 1.25em" data-wow-delay=".5s">
                        <div class="new-top" style="height:300px;">
                            <a href="/chi-tiet/<%= pro.name%>.<%= pro._id%>.<%= pro.cateId%>"><img src="upload/<%= pro.img%>" class="img-responsive" alt="" /></a>
                            <div class="new-text">
                                <ul>
                                    <li><a class="item_add" href="/add-cart.<%= pro._id %>"> Thêm vào giỏ hàng</a></li>
                                    <li><a href="/chi-tiet/<%= pro.name%>.<%= pro._id%>.<%= pro.cateId%>">Xem chi tiết </a></li>
                                    <li><a href="san-pham">Đi đến cửa hàng </a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="new-bottom">
                            <h5><a class="name" href="/chi-tiet/<%= pro.name%>.<%= pro._id%>.<%= pro.cateId%>"><%=pro.name%> </a></h5>
                            <div class="rating">
                                <span class="on">☆</span>
                                <span class="on">☆</span>
                                <span class="on">☆</span>
                                <span class="on">☆</span>
                                <span>☆</span>
                            </div>
                            <%if( pro.promotion <= 0 ) { %>
                                <div class="ofr">
                                    <p style = "width:100%;">
                                    Giá: 
                                    <span class="item_price" id="price<%=pro.price%>" style="color: #FF590F;"><%=pro.price%> VNĐ</span>
                                </p>
                                </div>
                                <% } else { %>
                                <div class="ofr">
                                    <p>
                                    Giá: 
                                    <del><%=pro.price%> VNĐ</del>
                                    <span class="item_price" id="price<%=pro.price%>" style="color: #FF590F;"><%=(pro.price)-((pro.price) * ((pro.promotion) /100))%> VNĐ</span>
                                </p>
                                </div>
                                <% } %>
                        </div>
                    </div>
                    <% }) %>
                    <div class="clearfix"> </div>
                </div>
            </div>
            <!--//related-products-->
        </div>
    </div>
    <!--//single-page-->
    <!--footer-->
    <% include ../layout/footer.ejs %>
    <!--search jQuery-->
    <script src="customer/js/main.js"></script>
    <!--//search jQuery-->
    <!--smooth-scrolling-of-move-up-->
    <script type="text/javascript">
        $(document).ready(function () {

            var defaults = {
                containerID: 'toTop', // fading element id
                containerHoverID: 'toTopHover', // fading element hover id
                scrollSpeed: 1200,
                easingType: 'linear'
            };

            $().UItoTop({
                easingType: 'easeOutQuart'
            });

        });
    </script>
    <!--//smooth-scrolling-of-move-up-->
    <!--Bootstrap core JavaScript
    ================================================== -->
    <!--Placed at the end of the document so the pages load faster -->
    <script src="customer/js/bootstrap.js"></script>
</body>

</html>